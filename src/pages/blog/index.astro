---
import DefaultLayout from "../../layouts/DefaultLayout.astro";
const posts = await Astro.glob("./posts/*.mdx");
console.log(posts);
---

<DefaultLayout>
	<p>Post Archive:</p>
	<ul>
		{
			posts.map((post) => (
				<li>
					<a href={post.url}>{post.frontmatter.title}</a>
				</li>
			))
		}
	</ul>
</DefaultLayout>
